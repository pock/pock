target 'Pock' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # PockKit
  pod 'PockKit'
  
  # Pods for Pock
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'Magnet'
  pod 'Defaults'
  pod 'Preferences'
  
end
